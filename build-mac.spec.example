# -*- mode: python ; coding: utf-8 -*-


a = Analysis(
    ['main.py'],
    pathex=[],
    binaries=[],
    datas=[('SimSun.ttf', '.'), ('icon/favicon-mac.icns', '.')],
    hiddenimports=[],
    hookspath=[],
    hooksconfig={},
    runtime_hooks=[],
    excludes=[],
    noarchive=False,
    optimize=0,
)
pyz = PYZ(a.pure)

exe = EXE(
    pyz,
    a.scripts,
    [],
    exclude_binaries=True,
    name='rabpdf',
    debug=False,
    bootloader_ignore_signals=False,
    strip=False,
    upx=True,
    console=False,
    disable_windowed_traceback=False,
    argv_emulation=False,
    target_arch=None,
    entitlements_file=None,
    icon=['icon/favicon-mac.icns'],
)
coll = COLLECT(
    exe,
    a.binaries,
    a.datas,
    strip=False,
    upx=True,
    upx_exclude=[],
    name='rabpdf',
)
app = BUNDLE(
    coll,
    name='RabPDF.app',
    icon='icon/favicon-mac.icns',
    bundle_identifier='com.chandler.rabpdf',
    info_plist={
        'CFBundleShortVersionString': '1.0.2',
        'CFBundleVersion': '1.0.2',
    }
)
